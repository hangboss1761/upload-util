    "shelljs": "^0.8.3",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@types/node": "^13.9.0",
    "@types/semver": "^7.1.0",
    "@types/shelljs": "^0.8.6",
